  "protocol_handlers": [
    {
      "protocol": "web+tassili",
      "url": "/handler?url=%s"
    }
  ],
  "share_target": {
    "action": "/partager",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  }
